{{!-- Saber que encargado por fragmento cuenta con mas bonos --}}

<h3> Lista de los Encargados por región que cuentan con más bonos</h3>
<hr/>

<table class="table table-dark table-striped">
  <thead>
    <tr>
      <th scope="col">Región</th>
      <th scope="col">ID</th>
      <th scope="col">Primer nombre</th>
      <th scope="col">Apellido</th>
    </tr>
  </thead>
  <tbody>
  {{#each data}}
    <tr>
      <td> {{Territorio}} </td>
      <td> {{ID}} </td>
      <td> {{FirstName}} </td>
      <td> {{LastName}} </td>
    </tr>
  {{/each}}
  </tbody>
</table>